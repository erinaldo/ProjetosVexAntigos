                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText> SELECT FL.NOME NOMEDAFILIAL,   COUNT(*) NOTASFISCAIS,  NF.PRAZOUTILIZADO--,   CONVERT(DECIMAL(5,2)  ,AVG(NF.PRAZOUTILIZADO)) AS MEDIA   
 FROM DOCUMENTO NF   INNER JOIN CADASTRO REME ON (REME.IDCADASTRO = NF.IDREMETENTE)   
 INNER JOIN CADASTRO DEST ON (DEST.IDCADASTRO = NF.IDDESTINATARIO)   
 INNER JOIN CIDADE DESTCID ON (DESTCID.IDCIDADE = DEST.IDCIDADE)   
 INNER JOIN ESTADO DESTEST ON (DESTEST.IDESTADO = DESTCID.IDESTADO)   
 INNER JOIN FILIAL FL ON (FL.IDFILIAL = NF.IDFILIAL)   
 WHERE   NF.TIPODEDOCUMENTO = 'NOTA FISCAL'   
 AND NF.DATADEEMISSAO BETWEEN  CONVERT(DATETIME, '6/09/2010 00:00:00', 103)
 AND CONVERT(DATETIME, '30/09/2010 00:00:00', 103) AND NF.DATADECONCLUSAO IS NOT NULL   
 AND NF.PRAZOUTILIZADO IS NOT NULL   AND (NF.IDREMETENTE IN (444,446,445)  OR NF.IDDESTINATARIO IN (444,446,445)  OR NF.IDCLIENTE IN (444,446,445) ) 
 AND FL.NOME='GUARULHOS' 
 GROUP BY FL.NOME, NF.PRAZOUTILIZADO   
 ORDER BY FL.NOME </CommandText>
