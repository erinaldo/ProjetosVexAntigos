                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT DISTINCT  OCO.CODIGO, LTRIM(RTRIM(OCO.NOME)) NOME, COUNT(*) Notas  
FROM DOCUMENTO DOC WITH(NOLOCK)   
LEFT JOIN DOCUMENTOOCORRENCIA DOCOCO WITH(NOLOCK) ON(DOCOCO.IDDOCUMENTOOCORRENCIA = DOC.IDDOCUMENTOOCORRENCIA)   
LEFT JOIN OCORRENCIA OCO WITH(NOLOCK) ON(DOCOCO.IDOCORRENCIA = OCO.IDOCORRENCIA)   
WHERE (DOC.IDREMETENTE in(@cli) OR DOC.IDCLIENTE IN(@cli) OR DOC.IDDESTINATARIO IN(@cli5))
AND DOC.TIPODEDOCUMENTO = 'NOTA FISCAL'  AND NOT DOC.DATADEENTRADA IS NULL 
AND DOC.CODIGODORECEXP IS NOT NULL  AND DOC.DATADECONCLUSAO IS NULL  
AND DOC.IDDOCUMENTOOCORRENCIA IS NOT NULL   
AND OCO.NOME IS NOT NULL AND OCO.CODIGO &lt;&gt;'01'   
GROUP BY OCO.CODIGO, OCO.Nome
ORDER BY OCO.CODIGO, OCO.Nome
</CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="false" AutogeneratedName="cli" ColumnName="" DataSourceName="" DataTypeServer="unknown" DbType="String" Direction="Input" ParameterName="@cli" Precision="0" Scale="0" Size="0" SourceColumn="" SourceColumnNullMapping="false" SourceVersion="Current" />
