                      <Parameter AllowDbNull="false" AutogeneratedName="CODIGO" ColumnName="Codigo" DataSourceName="Nasha.dbo.ProdutoCliente" DataTypeServer="varchar(20)" DbType="AnsiString" Direction="Input" ParameterName="@CODIGO" Precision="0" ProviderType="VarChar" Scale="0" Size="20" SourceColumn="Codigo" SourceColumnNullMapping="false" SourceVersion="Current" />
