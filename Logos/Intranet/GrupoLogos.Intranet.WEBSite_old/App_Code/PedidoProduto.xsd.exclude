                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT 
DOC.NUMERO,
PC.DESCRICAO, 
PC.CODIGO, 
DOC.DATADOMOVIMENTO, 
DI.QUANTIDADE, 
DI.IDUSUARIO, 
USU.NOME, 
CADDEST.RAZAOSOCIALNOME DESTINATARIO
FROM DOCUMENTO DOC
INNER JOIN DOCUMENTOITEM DI ON DI.IDDOCUMENTO = DOC.IDDOCUMENTO
INNER JOIN PRODUTOEMBALAGEM PE ON PE.IDPRODUTOEMBALAGEM = DI.IDPRODUTOEMBALAGEM
INNER JOIN PRODUTOCLIENTE PC ON PC.IDPRODUTOCLIENTE = PE.IDPRODUTOCLIENTE
INNER JOIN CADASTRO CADDEST ON CADDEST.IDCADASTRO = DOC.IDDESTINATARIO
INNER JOIN USUARIO USU ON  USU.IDUSUARIO = DI.IDUSUARIO
WHERE TIPODEDOCUMENTO='PEDIDO' 
AND ORIGEM='INTERNET' 
AND CODIGO = @CODIGO
ORDER BY DOC.NUMERO, PC.DESCRICAO 

</CommandText>
