                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT  
R.RAZAOSOCIAL REMETENTE,  
D.RAZAOSOCIAL DESTINATARIO,  
CNH.SERIEDOCONHECIMENTO SERIE,  
CNH.CONHECIMENTO NOTAFISCAL,  
CNH.VOLUMES,  
CNH.PESO,  
CNH.VALORDANOTA,  
CNH.DATADECADASTRO,  
CNH.DATADEEMISSAO  
FROM CONHECIMENTO CNH  
INNER JOIN CLIENTE R ON (R.CNPJ = CNH.CNPJREMETENTE)   
INNER JOIN CLIENTE D ON (D.CNPJ = CNH.CNPJDESTINATARIO)   
INNER JOIN CONHECIMENTOAEMBARCAR CEMB ON (CEMB.FILIALLANCAMENTO = CNH.FILIALLANCAMENTO AND CEMB.CONTROLE = CNH.CONTROLE)   
WHERE      CEMB.SITUACAO IN ('AGUARDANDO ENTREGA', 'AGUARDANDO EMBARQUE')   
AND CEMB.FILIALATUAL = @FL  
AND DATEDIFF(DAY, CNH.DATADECADASTRO, GETDATE()) &gt; @DIAS </CommandText>
                    <Parameters>
                      <Parameter AllowDbNull="false" AutogeneratedName="FL" ColumnName="FilialAtual" DataSourceName="Logos.dbo.ConhecimentoAEmbarcar" DataTypeServer="char(2)" DbType="AnsiString" Direction="Input" ParameterName="@FL" Precision="0" ProviderType="Char" Scale="0" Size="2" SourceColumn="FilialAtual" SourceColumnNullMapping="false" SourceVersion="Current" />
                      <Parameter AllowDbNull="false" AutogeneratedName="DIAS" ColumnName="" DataSourceName="" DataTypeServer="unknown" DbType="Decimal" Direction="Input" ParameterName="@DIAS" Precision="0" Scale="0" Size="0" SourceColumn="" SourceColumnNullMapping="false" SourceVersion="Current" />
                    </Parameters>
