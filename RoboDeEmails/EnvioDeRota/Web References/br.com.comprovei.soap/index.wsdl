      <xsd:import schemaLocation="http://soap.comprovei.com.br/importQueue/v2/schemas/getImportProtocolStatus.xsd" namespace="WebServiceComprovei:getImportProtocolStatus" />
      <xsd:element name="Credenciais" type="wsc:Credenciais" />
      <xsd:complexType name="Credenciais">
        <xsd:sequence>
          <xsd:element name="Usuario" type="xsd:string" />
          <xsd:element name="Senha" type="xsd:string" />
        </xsd:sequence>
      </xsd:complexType>
    </xsd:schema>
  </types>
  <message name="uploadDocumentsRequest">
    <part name="parameters" element="docs:uploadDocuments" />
  </message>
  <message name="uploadDocumentsResponse">
    <part name="parameters" element="docs:uploadDocumentsResponse" />
  </message>
  <message name="uploadDocumentsCredenciais">
    <part name="Credenciais" element="wsc:Credenciais" />
  </message>
  <message name="uploadRouteRequest">
    <part name="parameters" element="routes:uploadRoute" />
  </message>
  <message name="uploadRouteResponse">
    <part name="parameters" element="routes:uploadRouteResponse" />
  </message>
  <message name="uploadRouteCredenciais">
