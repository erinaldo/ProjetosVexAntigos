                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT DISTINCT U.IDUSUARIO, U.NOME, U.LOGIN, U.SENHA,
CD.NOME,
CD.IDCLIENTEDIVISAO, PC.CODIGO, PC.DESCRICAO,  
(SELECT NOME FROM USUARIO UU WHERE UU.IDUSUARIO = U.IDPERFIL) PERFIL
FROM USUARIO U
INNER JOIN USUARIOCLIENTE UC ON UC.IDUSUARIO = UC.IDUSUARIO
INNER JOIN USUARIOCLIENTEDIVISAO UCD ON UCD.IDUSUARIOCLIENTE = UC.IDUSUARIOCLIENTE
INNER JOIN CLIENTEDIVISAO CD ON CD.IDCLIENTEDIVISAO = UCD.IDCLIENTEDIVISAO
INNER JOIN PRODUTOCLIENTE PC ON PC.IDCLIENTE = UC.IDCLIENTE
WHERE UC.IDCLIENTE = 1
AND U.TIPO='USUARIO'
AND U.TIPODESISTEMA='WEB'
ORDER BY U.NOME</CommandText>
