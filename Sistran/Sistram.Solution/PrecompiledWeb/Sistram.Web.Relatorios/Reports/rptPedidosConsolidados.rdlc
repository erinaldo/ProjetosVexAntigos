      <Query>
        <DataSourceName>SistranWebGrupoLogos</DataSourceName>
        <CommandText>SELECT 
CAST(DOC.DATADEEMISSAO AS DATE) DATA,  
PC.CODIGO, 
pc.Descricao,
SUM(DIT.QUANTIDADE) QTD,
DEST.CNPJCPF CNPJCPF_DESTINATARIO, 
ISNULL(DEST.RAZAOSOCIALNOME, DEST.FANTASIAAPELIDO) AS DESTINATARIO
FROM DOCUMENTO DOC
INNER JOIN DOCUMENTOITEM DIT ON DIT.IDDOCUMENTO = DOC.IDDOCUMENTO
INNER JOIN PRODUTOEMBALAGEM PEM ON PEM.IDPRODUTOEMBALAGEM = DIT.IDPRODUTOEMBALAGEM
INNER JOIN PRODUTOCLIENTE PC ON PC.IDPRODUTOCLIENTE = PEM.IDPRODUTOCLIENTE
INNER JOIN CADASTRO DEST ON DEST.IDCADASTRO = DOC.IDDESTINATARIO
WHERE 
DOC.TIPODEDOCUMENTO='PEDIDO' 
AND DOC.ATIVO='SIM' 
AND DATADECANCELAMENTO IS NULL
GROUP BY 
CAST(DOC.DATADEEMISSAO AS DATE),  
PC.CODIGO, 
pc.Descricao,
DEST.CNPJCPF , 
ISNULL(DEST.RAZAOSOCIALNOME, DEST.FANTASIAAPELIDO) 
ORDER BY DATA DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
