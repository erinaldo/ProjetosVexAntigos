              <Mapping SourceColumn="NOTASFISCAIS_NAO_ENTREGUE_NO_PRAZO" DataSetColumn="NOTASFISCAIS_NAO_ENTREGUE_NO_PRAZO" />
              <Mapping SourceColumn="TRANSITTIME" DataSetColumn="TRANSITTIME" />
              <Mapping SourceColumn="MEDIA_DOS_DIAS_NAO_ENTREGUE" DataSetColumn="MEDIA_DOS_DIAS_NAO_ENTREGUE" />
              <Mapping SourceColumn="MEDIA_DOS_DIAS_NAO_ENTREGUE_ATRASO" DataSetColumn="MEDIA_DOS_DIAS_NAO_ENTREGUE_ATRASO" />
              <Mapping SourceColumn="MEDIA_DOS_DIAS_NAO_ENTREGUE_NO_PRAZO" DataSetColumn="MEDIA_DOS_DIAS_NAO_ENTREGUE_NO_PRAZO" />
            </Mappings>
