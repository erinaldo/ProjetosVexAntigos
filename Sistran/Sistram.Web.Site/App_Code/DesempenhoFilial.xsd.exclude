          <Connection AppSettingsObjectName="Web.config" AppSettingsPropertyName="SistranWebGrupoLogos" ConnectionStringObject="" IsAppSettingsProperty="true" Modifier="Assembly" Name="SistranWebGrupoLogos (Web.config)" ParameterPrefix="@" PropertyReference="AppConfig.System.Configuration.ConfigurationManager.0.ConnectionStrings.SistranWebGrupoLogos.ConnectionString" Provider="System.Data.SqlClient" />
        </Connections>
        <Tables>
          <TableAdapter BaseClass="System.ComponentModel.Component" DataAccessorModifier="AutoLayout, AnsiClass, Class, Public" DataAccessorName="FilialTableAdapter" GeneratorDataComponentClassName="FilialTableAdapter" Name="Filial" UserDataComponentName="FilialTableAdapter">
            <MainSource>
              <DbSource ConnectionRef="SistranWebGrupoLogos (Web.config)" DbObjectName="GrupoLogosTeste.dbo.Filial" DbObjectType="Table" FillMethodModifier="Public" FillMethodName="Fill" GenerateMethods="Both" GenerateShortCommands="false" GeneratorGetMethodName="GetData" GeneratorSourceName="Fill" GetMethodModifier="Public" GetMethodName="GetData" QueryType="Rowset" ScalarCallRetval="System.Object, mscorlib, Version=2.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" UseOptimisticConcurrency="false" UserGetMethodName="GetData" UserSourceName="Fill">
                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT  
 FL.NOME NOMEDAFILIAL,
 SUM(CASE WHEN  (NF.DATADECONCLUSAO IS NOT NULL)  THEN 1 ELSE 0 END) NOTASFISCAIS_ENTREGUE,  
 AVG(CAST((ISNULL(DATEDIFF(DAY,  NF.DATADEEMISSAO, NF.DATADECONCLUSAO),0)) AS NUMERIC(10,3))) LEADTIME,
 SUM(CASE WHEN (NF.DATADECONCLUSAO IS NULL)  THEN 1 ELSE 0 END) NOTASFISCAIS_NAO_ENTREGUE,
 SUM(CASE WHEN (NF.DATADECONCLUSAO IS NULL) AND (DATEDIFF(D, NF.DATAPLANEJADA,GETDATE())&gt;1)  THEN 1 ELSE 0 END) NOTASFISCAIS_NAO_ENTREGUE_ATRASO,   
 SUM(CASE WHEN (NF.DATADECONCLUSAO IS NULL) AND (DATEDIFF(D, NF.DATAPLANEJADA,GETDATE())&lt;=1)  THEN 1 ELSE 0 END) NOTASFISCAIS_NAO_ENTREGUE_NO_PRAZO,   
 AVG(ISNULL(NF.PRAZOUTILIZADO,0) - ISNULL(NF.DIASOCORRENCIACLIENTE,0)) AS TRANSITTIME,
 AVG(CAST((
 CASE WHEN (NF.DATADECONCLUSAO IS NULL) THEN
   DATEDIFF(D,  NF.DATAPLANEJADA, GETDATE()) ELSE
   DATEDIFF(D,  NF.DATAPLANEJADA, NF.DATADECONCLUSAO)
 END) AS NUMERIC(10,2)))  MEDIA_DOS_DIAS_NAO_ENTREGUE,
 AVG(CAST((
 CASE WHEN (NF.DATADECONCLUSAO IS NULL) AND DATEDIFF(D,  NF.DATAPLANEJADA, GETDATE())&gt;1 THEN
   DATEDIFF(D,  NF.DATAPLANEJADA, GETDATE()) ELSE
   DATEDIFF(D,  NF.DATAPLANEJADA, NF.DATADECONCLUSAO)
 END) AS NUMERIC(10,2)))  MEDIA_DOS_DIAS_NAO_ENTREGUE_ATRASO,
 AVG(CAST((
 CASE WHEN (NF.DATADECONCLUSAO IS NULL) AND DATEDIFF(D,  NF.DATAPLANEJADA, GETDATE())&lt;=1 THEN
   DATEDIFF(D,  NF.DATAPLANEJADA, GETDATE()) ELSE
   DATEDIFF(D,  NF.DATAPLANEJADA, NF.DATADECONCLUSAO)
 END) AS NUMERIC(10,2)))  MEDIA_DOS_DIAS_NAO_ENTREGUE_NO_PRAZO
   
 FROM DOCUMENTO NF  
 INNER JOIN CADASTRO REME ON (REME.IDCADASTRO = NF.IDREMETENTE)   
 INNER JOIN CADASTRO DEST ON (DEST.IDCADASTRO = NF.IDDESTINATARIO)   
 INNER JOIN CIDADE DESTCID ON (DESTCID.IDCIDADE = DEST.IDCIDADE)   
 INNER JOIN ESTADO DESTEST ON (DESTEST.IDESTADO = DESTCID.IDESTADO)   
 INNER JOIN FILIAL FL ON (FL.IDFILIAL = NF.IDFILIAL)     
 WHERE   NF.TIPODEDOCUMENTO = 'NOTA FISCAL'  
 AND NF.DATADEEMISSAO BETWEEN  CONVERT(DATETIME, '01/03/2011 00:00:00', 103)
AND CONVERT(DATETIME, '31/03/2011 00:00:00', 103)  AND (NF.IDREMETENTE IN
(444,446,445,62694)  OR NF.IDDESTINATARIO IN (444,446,445,62694)  OR
NF.IDCLIENTE IN (444,446,445,62694) ) 
 
 GROUP BY FL.NOME   
 ORDER BY AVG(NF.PRAZOUTILIZADO) 
</CommandText>
                    <Parameters />
                  </DbCommand>
                </SelectCommand>
              </DbSource>
            </MainSource>
